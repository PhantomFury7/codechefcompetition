#include <stdio.h>

int main(void) {
    int testcase;
    scanf("%d", &testcase );
	for(int i=0;i<testcase;i++)
	{
	    int cost;
        scanf("%d",& cost );
        int tc;
        tc=cost*4;
        if(tc<=1000)
        {
            printf("yes\n");
        }
        else(printf("no\n"));
	}
	return 0;
}
